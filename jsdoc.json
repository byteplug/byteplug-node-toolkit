{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [
            "payload/src",
            "endpoints/src"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "tutorials": "documentation",
        "destination": "./out/"
    }
}
